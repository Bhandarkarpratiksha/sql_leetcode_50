#report making - https://www.hackerrank.com/challenges/the-report/problem?isFullScreen=true
# not actual foreign key is provided 
SELECT
    CASE
        WHEN G.GRADE < 8 THEN NULL
        ELSE S.NAME
    END AS NAME,
    G.GRADE,
    S.MARKS
FROM STUDENTS S
JOIN GRADES G
    ON S.MARKS BETWEEN G.MIN_MARK AND G.MAX_MARK
ORDER BY
    G.GRADE DESC,
    CASE
        WHEN G.GRADE >= 8 THEN S.NAME
        ELSE NULL
    END ASC,
    CASE
        WHEN G.GRADE < 8 THEN S.MARKS
        ELSE NULL
    END ASC;
